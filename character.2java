import java.util.*;

class character
{
	public static void main(String arg[])
	{
		char[] name = new char[5];
		Scanner sc = new Scanner(System.in);
		int temp;

		for(int a =0;a<name.length;a++)
		{
			System.out.println("Enter char "+a+": ");
			name[a] = sc.next().charAt(0);
			temp=(int)name[a]+32;
			name[a] = (char) temp;
		}
		System.out.println();
		for(int a =0;a<name.length;a++)
		{
			System.out.print(name[a]+" ");
			
		}
		
		
		
	}
}

